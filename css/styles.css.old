@import url('https://fonts.googleapis.com/css2?family=Lora:ital,wght@0,400;0,500;0,600;0,700;1,400;1,500;1,600;1,700&family=Rubik:ital,wght@0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap');

*{
    margin: 0;
    padding: 0;
    box-sizing: border-box;
}

ul, li, a, h1, h2, p, body, header, section, footer, .container, .container-fluid, .row, .col{
    margin: 0;
    padding: 0;
}

html{
    font-size: 100%;
    scroll-padding-top: 4rem;
    overflow-x: hidden;
}

body{
    line-height: initial;
    width: 100%;
    height:100%;
    overflow-x: hidden;
    color:#191D32;
    background-color:white;
    font-family: 'Rubik', verdana, sans-serif;
    font-weight: 400;
    font-size: 1rem;
}

a, ul{
    list-style: none;
    text-decoration: none;
}

a:hover{
    color: white;
}

p, a, ul, h1, h2, table, label, input{
    transition: font-size 200ms;
}   

/* INICIO HEADER (logo y nav) */
.header{ 
    display: flex;
    flex-flow: row wrap;
    justify-content: flex-end; 
    align-items: flex-end; 
    color: #ffffff;
    width: 100%;
    position: fixed; 
    top: 0;
    z-index: 1;
    background-color: rgba(14, 16, 26, 0.5);
    height: 4rem;
    border-bottom: .1rem solid white;
}

.header__logo{     
    margin-right: auto;
    margin-left: .5rem;  
    font-family: 'Rubik', verdana, sans-serif;
    font-weight: 900;
    font-style: italic;
    font-size:3.5rem;    
    color: white;
    text-shadow: 0 0 .4rem #f81298;
    transition: 250ms;
   
}
    
.header__logo:hover, .header__logo:focus{
    color: #f81298;
    text-shadow: 0 0 .8rem #f81298;
    animation: 1000ms logoAnimation linear infinite alternate-reverse;
}

.header__logo:active{
    animation:500ms logoAnimation linear infinite alternate-reverse;
}

.header__burgerIcon{
    font-size: 0;
    margin-right: auto;
    margin-left: .5rem;
    color: white;
    display: none;
}

.nav__list{ 
    display: flex;
    flex-flow: row wrap;
    justify-content: space-evenly;
    font-family: 'Rubik', verdana, sans-serif;
    font-weight: 800;
    font-size:2.2rem;
    text-shadow: 0 0 .1rem #191D32;   
}

.nav__link{
    color: #ffffff;    
    margin: 0 .5rem;
}

.nav__link__arrow{
    display: none;
}

.nav__link:hover, .nav__link:focus{
    text-shadow: 0 0 .4rem #f81298;
}

.nav__link--active{
    text-shadow: 0 0 .4rem #f81298;
}

.nav__submenu{
    position: relative;
}

.nav__submenu__list{
    position: absolute;
    top: 2.5rem;
    text-align: right;
    width: 100%;
    font-size: .7em;
    background-color: rgba(25, 29, 50, .9);
    border-radius: 0 0 0 calc(.4vw + .4vh);
    text-shadow: 0 0 .2rem #191D32; 
    display: none; 
}

.nav__submenu:hover .nav__submenu__list{
    display: flex;
    flex-flow: column wrap;
    align-items: flex-end;
    justify-content: flex-start;
}

.nav__whatsapp{  
    font-size: 4rem;
    background: transparent;
    color: #075E54; 
    text-shadow: none;
}

.nav__whatsapp:hover{
    color: #25D366;
    text-shadow: none;
}



/* FIN HEADER */


.section{          
    text-align: center;
    display: flex;
    flex-flow: column wrap;
    justify-content: space-evenly;
    align-items:center;
    align-content:center;
}

/* INICIO LANDING VIDEO OVERLAY */

.landing{
    width: 100vw;
    height: calc(100vh - .5rem);
    min-width: 100vw;
    max-width: 100vh;
    overflow: hidden;
    position: relative;
}

.landing__video{
    width: 100%;   
    height: calc(100vh - .5rem);
    object-fit: cover;
}

.landing__overlay{
    width: 100%;
    height: calc(100vh - .5rem);
    position: absolute;
    top: 0;
    background: linear-gradient(270deg, #191D32,#3db8ff , #191D32);
    opacity:  60%; 
}

.landing_overlay__text{
    margin-top: calc(4rem + 1vw + 1vh);
    color: #191D32;
}

.text__p{
    font-family: 'Rubik', verdana, sans-serif;
    font-size: calc(1rem + 2vw);
    font-style: italic;
    line-height: 0;
    animation: titulo 2500ms 1; 
}

.text__h1{
    font-family: 'Rubik', verdana, sans-serif;
    font-weight: 900;
    font-size: calc(3.2rem + 6vw);
    letter-spacing: .5vw;    
} 

.text__h2{
    width: 100%;
    background-color:#191D32;
    color: white;
    font-family: 'Rubik', verdana, sans-serif;
    font-weight: 800;
    font-size: calc(1.6rem + 2.4vw);       
    padding-top: calc(2vw + 2vh);
    padding-bottom: calc(2vw + 2vh);
    margin: 0;
    text-shadow: 0 0 .4rem #f81298;
}

.text__h3{
    background-color:#191D32;
    color: white;
    font-family: 'Rubik', verdana, sans-serif;
    font-weight: 800;
    width: 100%;
    font-size: calc(1.6rem + 2vw); 
    text-shadow: 0 0 .4rem #f81298;
    text-align: center;
    margin-top: calc(2vw + 2vh);
}

.lineaGradiente{
    width: 100%;
    height: .5rem;
    background: linear-gradient(270deg, #3db8ff, #f81298, #3db8ff);
    background-size: 500% 500%;
    animation: lineaGradienteAnimation 20000ms linear infinite;
}

/* FIN LANDING VIDEO OVERLAY */



.bigButton{
    font-family: 'Rubik', verdana, sans-serif;
    font-weight: 900;
    font-size: calc(1.2rem + 1.8vw + 1.8vh); 
    background: linear-gradient(270deg, #3db8ff, #f81298, #3db8ff);
    background-size: 200% 200%;
    color: white;
    border-radius: calc(1vw + 1vh); 
    padding: calc(.5rem + .5vw + .5vh);
    margin: calc(2vw + 2vh) auto calc(2vw + 2vh) auto;   
    cursor: pointer;
    display: inline-block;
    border: none;
    outline: none;
    box-shadow: 0 0 .5rem 0 #191D32;
    text-shadow: 0; 
    transition: 200ms;
    max-width: 90%;
    text-align: center;
}

.bigButton:hover, .bigButton:focus{
    padding: calc(.25rem + .5vw + .5vh);    
    animation: 1000ms logoAnimation linear infinite alternate-reverse, 5000ms lineaGradienteAnimation linear infinite alternate-reverse, buttonBoxShadow 2000ms linear infinite alternate-reverse;
    transition: 200ms;      
    margin: calc(.25rem + 2vw + 2vh) auto calc(.25rem + 2vw + 2vh) auto;    
}

.bigButton:active{
    animation: 5000ms logoAnimation linear infinite alternate-reverse, 2500ms lineaGradienteAnimation linear infinite alternate-reverse, buttonBoxShadow 1000ms linear infinite alternate-reverse;
    box-shadow: 0 0 .25rem 0;
    padding: calc(.125rem + .5vw + .5vh);
    transition: 100ms;
    margin: calc(.375rem + 2vw + 2vh) auto calc(.375rem + 2vw + 2vh) auto; 
}

/* INICIO GALERIA */

.carouselGallery{
    margin: calc(4vw + 4vh) auto calc(2vw + 2vh) auto;
}

.carouselGallery__imgContainer{
    padding: 0;
}

.carouselGallery__img{
    border: .5rem solid transparent;
    border-image: linear-gradient(270deg, #3db8ff, #f81298, #3db8ff);
    border-image-slice: 1;
    cursor: pointer;       
    transition: 5ms;   
    margin: auto;
    width: 90%;
    max-width: min(350px, 22vw);
    min-width: 280px;
    height: auto; 
    box-sizing: content-box; 
    
    /* inicio antialiasing */
    outline: 1px solid transparent;  
    backface-visibility: hidden;
    transform: translateZ(0);
    will-change: transform;
    perspective: 1000;
    /* fin antialiasing */
    
    transform: skew(-5deg);
}

.carouselGallery__prev__icon,  .carouselGallery__next__icon{
    opacity: .5;
    background-color: rgb(25, 29, 50, .5);
}

.carouselGallery__prev{
    left: -4%;
}

.carouselGallery__next{
    right: -4%;
}

.carouselGalleryB, .carouselGalleryC{
    display:none;
}

.artistas{
    margin: calc(2vw + 2vh) auto calc(2vw + 2vh) auto;
    display: grid;
    grid-template-columns: 1fr 1fr 1fr 1fr;
    justify-content: center;
    align-content: center;
    justify-items: center;
    align-items: center;
}

.artistas__tatuador{
    width: 70%;
    height: auto;     
    position: relative;
    margin: calc(1vw + 1vh) auto calc(2vw + 2vh) auto;
}

.artistas__tatuador__img{
    border-radius: calc(.25vw + .25vh) calc(.25vw + .25vh) 0 0;
    width: 100%;
    height: auto;
}

.artistas__tatuador::after {
    content: '';
    position: absolute;
    top: clamp(-.6rem, -1rem + .3vw, -.5rem); 
    bottom: clamp(-.6rem, -1rem + .3vw, -.5rem);
    left: clamp(-.6rem, -1rem + .3vw, -.5rem); 
    right: clamp(-.6rem, -1rem + .3vw, -.5rem);
    background: linear-gradient(270deg, #3db8ff, #f81298, #3db8ff);
    background-size: 200% 200%;   
    z-index: -1;
    border-radius: calc(.5vw + .5vh);
    transition: 200ms;     
}

.artistas__tatuador:hover::after, .artistas__tatuador:focus::after{
    top: clamp(-.4rem, -1rem + .4vw, -.35rem); 
    bottom: clamp(-.4rem, -1rem + .4vw, -.35rem);
    left: clamp(-.4rem, -1rem + .4vw, -.35rem); 
    right: clamp(-.4rem, -1rem + .4vw, -.35rem);   
    animation: lineaGradienteAnimation 2000ms linear infinite alternate-reverse;   
}

.artistas__tatuador:active::after, .artistas__tatuador:active::after{
    top: clamp(-.3rem, -1rem + .5vw, -.25rem); 
    bottom: clamp(-.3rem, -1rem + .5vw, -.25rem);
    left: clamp(-.3rem, -1rem + .5vw, -.25rem); 
    right: clamp(-.3rem, -1rem + .5vw, -.25rem);  
    animation: lineaGradienteAnimation 1000ms linear infinite alternate-reverse; 
}

.tatuador__details{
    display: flex;
    flex-flow: column wrap;
}

.tatuador__details__h3{
	padding-top: calc(.2rem + .4vw + .4vh);
    height: 50%;
    background-color: #3db8ff;
    color: #191d32;
    margin: 0;
}

.tatuador__details__p{
	padding-bottom: calc(.2rem + .4vw + .4vh);
    height: 50%;   
    background-color: #3db8ff;
    color: #191d32;
    border-radius: 0 0 calc(.25vw + .25vh) calc(.25vw + .25vh);
    font-size: 1.2rem;
}

.videoGallery{
    margin: calc(4vw + 4vh) auto calc(2vw + 2vh) auto;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-evenly;
    align-items: center;  
    align-content: center;
   
    width: 100%;
}

.videoGallery__video{
    margin: auto;
    border-radius: 0 0 calc(.5vw + .5vh) calc(.5vw + .5vh);
    width: 24%;
    height: auto;     
    cursor: pointer;
    box-shadow: 0px .5rem 1rem -.5rem #191D32;
    border: 0px solid transparent;
    border-top: 1rem solid transparent;
    border-image: linear-gradient(270deg, #3db8ff, #f81298, #3db8ff);
    border-image-slice: 1;    
}

.videoGallery__video:nth-child(odd):hover{
    box-shadow: 0px .5rem 1rem -.5rem #3db8ff;
}

.videoGallery__video:nth-child(even):hover{
    box-shadow: 0px .5rem 1rem -.5rem #f81298;
}

.studioGallery{
    width: 100%;
    margin: calc(2vw + 2vh) auto calc(2vw + 2vh) auto;
    display: grid;
    grid-template-columns: 1fr 1fr 1fr 1fr;
    grid-template-rows: auto;
    justify-content: center;
    align-content: center;
    align-items: center;
    justify-items: center;      
}

.gallery{
    width: 100%;
    margin: calc(4vw + 4vh) auto calc(2vw + 2vh) auto;
    display: grid;
    grid-template-columns: 1fr 1fr 1fr 1fr;
    grid-template-rows: auto;
    justify-content: center;
    align-content: center;
    align-items: center;   
    justify-items: center;
}

.gallery__img{
    width: 70%;
    min-width: 300px;
    max-width: 100%;
    height: auto;
    margin: 2vw;  
    border: 0px solid transparent;
    border-top: 1rem solid transparent;
    border-image: linear-gradient(270deg, #3db8ff, #f81298, #3db8ff);
    border-image-slice: 1;
    box-shadow: 0px .5rem 1rem -.5rem #191D32;
    cursor: pointer;       
    transition: 5ms;
    box-sizing: content-box; 
}

.gallery__img:hover, .carouselGallery__img:hover, .gallery__img:focus, .carouselGallery__img:focus{
    border: .5rem solid; 
    animation: borderAnimation 2000ms linear infinite;
    box-shadow: none;    
}

.gallery__img:active{
    border: .38rem solid;  
    animation: borderAnimation 1500ms linear infinite;
    transform: scale(1.2);
}

/* FIN GALERIA */

/* INICIO FAQ */
.faq{
    margin: calc(4vw + 4vh) auto calc(2vw + 2vh) auto;
    font-family: 'Rubik', verdana, sans-serif;
    font-weight: 400;
    font-size: calc(.8rem + .28vw + .28vh);
    background-color: white;
    color: #191D32;   
    text-align: left;
    display: flex;
    flex-flow: column wrap;
    justify-content: center;
    align-items: center;
    text-align: left;
    line-height: 1.4;
}

.faq__pregunta{
    cursor: pointer;
    margin-bottom: calc(1vw + 1vh);
    width: 100%;
}

.faq__respuesta{
    border: none;
    font-family: 'Rubik', verdana, sans-serif;
    font-weight: 400;
    width: 100%;
    font-size: 1.1em;
    margin: 0 auto calc(2vw + 2vh) auto;  
}   

.text__h3--faq{
    font-size: 1.4em;
    font-weight: 600;
    font-style: italic;
    padding: .5rem;
    margin: 0;
    text-align: left;  
}


.faq__enviaDiseño{
    font-weight: bold;
    color: #191D32;
 }

 .faq__enviaDiseño:hover, .faq__enviaDiseño:focus{
    color: #191D32;
    text-shadow: 0 0 .4rem #f81298;
 }

.faq__mediosPagoImg{
    display: block;
    margin: 1vw auto 0 auto;
    max-width: 80%;
    min-width: 420px;
    height: auto;
}

.faq__piercingTable{
    border-radius: calc(.5vw + .5vh);
    margin: 0 auto 1vw auto;
    text-align: center;
    width: 80%;
    height: auto;
    min-width: 420px;
    font-size: 1.1em; 
    border-collapse: collapse;
    background-color: #191D32;
    color: white;  
}

.faq__piercingTable__th{
    text-shadow: 0 0 .4rem #f81298;
    border-bottom: .1em solid white;
}

/*  FIN FAQ */


/* INICIO CONTACTO (gmap e info) */




/* FIN CONTACTO */




/* inicio grid board (tablero arriba del footer */
.gridBoard{
    margin-top: calc(2vw + 2vh);
    width: 100vw;
    height: 10vw;    
    display: grid;
    grid-template-columns: repeat(10, 1fr);
    grid-template-rows: auto;
    cursor: pointer;
}


.gridBoard__keyA{
    background-color: #f81298;
    width: 100%;
    height: 100%;
    opacity: .5;
    transition: 500ms;
}

.gridBoard__keyB{
    background-color: #191D32;
    width: 100%;
    height: 100%;
    opacity: .5;
    transition: 500ms;
}

.gridBoard__keyA:hover{
    opacity: .75;
}

.gridBoard__keyB:hover{
    opacity: .75;
}

.gridBoard__keyA:active{
    opacity: 1;
}

.gridBoard__keyB:active{
    opacity: 1;
}
/* fin grid board */

/* INICIO FOOTER */
.footer{  
    background-color: #0e101a;
    color: white;
    text-align: center;
    font-family: 'Lora', 'Times New Roman', serif;
    font-weight: 400;
    width: 100%;
    display: flex;
    flex-flow: column wrap;
    justify-content: center;
}

.footer__list{
    margin-left: auto;
    margin-right: auto;
    display: grid;
    grid-template-columns: 1fr 1fr 1fr 1fr;
    grid-template-rows: auto;
    justify-content: space-evenly;
    width: 100%;
}


.footer__list__link{
    color: #ffffff;
    font-size: calc(10rem + 4vw);
    margin-left: 2vw;
    margin-right: 2vw;
}

.footer__santi{
    font-size: calc(.6rem + 1vw);
    align-self: flex-start;
    margin-left: .2rem;
}

.footer__copyright{
    border-top: .1rem solid white;
    font-size: calc(.6rem + 1vw);
    align-self: center;
    width: 100%;
}

.footer__list__link:hover, .footer__list__link:focus{
    text-shadow: 0 0 .4rem #f81298;
}

.footer__list__link:active{
    text-shadow: 0 0 .8rem #f81298;
}

/* FIN FOOTER */


/* INICIO KEYFRAMES */

@keyframes titulo{
    0% {
        letter-spacing: 5vw;
        font-size: 0;
    }
}

@keyframes lineaGradienteAnimation{
    0%{background-position:0% 50%}
    50%{background-position:100% 50%}
    100%{background-position:0% 50%}
}

@keyframes svgBorderAnimation{
    0%{border-image: linear-gradient(270deg, #f81298,#3db8ff);border-image-slice: 1;}
    10%{border-image: linear-gradient(306deg, #f81298,#3db8ff);border-image-slice: 1;}
    20%{border-image: linear-gradient(342deg, #f81298,#3db8ff);border-image-slice: 1;}
    30%{border-image: linear-gradient(18deg, #f81298,#3db8ff);border-image-slice: 1;}
    40%{border-image: linear-gradient(54deg, #f81298,#3db8ff);border-image-slice: 1;}
    50%{border-image: linear-gradient(90deg, #f81298,#3db8ff);border-image-slice: 1;}
    60%{border-image: linear-gradient(126deg, #f81298,#3db8ff);border-image-slice: 1;}
    70%{border-image: linear-gradient(162deg, #f81298,#3db8ff);border-image-slice: 1;}
    80%{border-image: linear-gradient(198deg, #f81298,#3db8ff);border-image-slice: 1;}
    90%{border-image: linear-gradient(234deg, #f81298,#3db8ff);border-image-slice: 1;}
    100%{border-image: linear-gradient(270deg, #f81298,#3db8ff);border-image-slice: 1;}
}

@keyframes borderAnimation{
    0%{border-image: linear-gradient(90deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
    10%{border-image: linear-gradient(108deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
    20%{border-image: linear-gradient(126deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
    30%{border-image: linear-gradient(144deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
    40%{border-image: linear-gradient(162deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
    50%{border-image: linear-gradient(180deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
    60%{border-image: linear-gradient(198deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
    70%{border-image: linear-gradient(216deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
    80%{border-image: linear-gradient(234deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
    90%{border-image: linear-gradient(252deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
    100%{border-image: linear-gradient(270deg, #3db8ff, #f81298, #3db8ff);border-image-slice: 1;}
}

@keyframes logoAnimation{
    0%{
        text-shadow: 0 0 .8rem #f81298;
    }
    100%{
        text-shadow: 0 0 1.8rem #f81298;
    }
}

@keyframes buttonBoxShadow{
    0%{box-shadow: 0 0 .5rem 0 #3db8ff}
    50%{box-shadow: 0 0 .5rem 0 #f81298}
}

/* FIN KEYFRAMES */


/* INICIO MEDIA QUERIES */

@media all and (max-width: 1600px){
    .gallery__img{
        margin: 2vw .4vw 2vw .4vw;
    }
 }
 
 @media all and (max-width: 1200px){
     .gallery{
         grid-template-columns: 1fr 1fr; 
     }
     .studioGallery{
         grid-template-columns: 1fr 1fr;      
     }
  
    .artistas{
        grid-template-columns: 1fr 1fr;
    }
    .footer__list{
        grid-template-columns: 1fr 1fr;
    }
    .videoGallery__video{
        width: 48%;
        margin: 1vh 0;
    }
 }

@media all and (max-width:1024px){ 
    .header__logo{
       margin-right: .5rem;
       margin-left: auto;
    }
    .header__burgerIcon{
        display: initial;
        font-size: 3.2rem;
        cursor: pointer;
        color: white;
        text-shadow: 0 0 .2rem #f81298;
    }
    .nav__link__arrow{
        display: inline; 
        font-size: 1.2rem;      
    }
    .nav__link__arrow:hover, .nav__link__arrow:focus{
        text-shadow: 0 0 .4rem #f81298;
    }
    .header__nav{
        position: fixed;
        top: 4rem;
        left: 0;  
        height: 0;
        overflow: hidden;
        width: 100%;
        transition: height 200ms;
    }
    .header__burgerIcon:hover{
        color: #f81298;
        text-shadow: 0 0 .4rem #f81298;
    }
    .header__burgerMenu:checked + .header__burgerIcon{
        color: #f81298;
        text-shadow: 0 0 .4rem #f81298;
    } 
    .header__burgerMenu:checked ~ .header__nav {
        height: 220px;
        transition: height 200ms;
    } 
    .nav__list{
        display: block;  
        width: 100%;        
        text-align: center;
        line-height: 2rem;
        font-size: 1.8rem;  
        text-shadow: 0 0 .4vw #0e101a80;    
        background-color: rgb(25, 29, 50, .9);       
    }
    .nav__submenu__list{
        top: 100%;
        width: 100%; 
        line-height: calc(1.5rem);
    }
    .nav__submenu:hover .nav__submenu__list{
        display: grid;
        grid-template-columns: 1fr 1fr;
        grid-template-rows: 1fr 1fr;
        justify-content: center;
        align-content: center;
        justify-items: center;
        align-items: center;
    }
    .escribinos{
        grid-row: 1 / 2;
        grid-column: 1 / 2;
        justify-self: right;
    }
    .mapa{
        grid-row: 2 / 3;
        grid-column: 1 / 2;
        justify-self: right;
    }
    .whats{
        grid-column: 2 / 3;
        grid-row: 1 / 3;
        justify-self: left;
    }
    .nav__whatsapp{
        font-size: 3rem; 
    }
    .about{
        font-size: calc(1rem + 2vw);
    } 
    .carouselGalleryA, .carouselGalleryC{
        display: none;
    }
    .carouselGalleryB{
        display: block;
    }
}

@media all and (max-width:768px){ 
    .contactInfo__gmap{
        width: 80vw;
    }
    .contactInfo__ul{
        width: 80vw;
    }

    .text__h1{
        font-size: calc(2.8rem + 6vw);
    }
    .text__p{
        font-size: calc(1.1rem + 2vw);
    }
}

@media all and (max-width: 600px){
    .gallery, .studioGallery{
        grid-template-columns: 1fr;       
    }
    .artistas{
        grid-template-columns: 1fr;
    }
    .carouselGalleryA, .carouselGalleryB{
        display: none;
    }
    .carouselGalleryC{
        display: block;
    }
    .videoGallery__video{
        width: 90%;
        margin: 1vh 0;
    }
}

@media all and (max-width: 450px){
    .footer__copyright, .footer__santi{
        font-size: calc(.5rem + 1vw);
    }
    .footer__list__link{
        margin-left: .5vw;
        margin-right: .5vw;
    }
  
    .faq__mediosPagoImg, .faq__piercingTable{
        min-width: 100%;
    }
}

@media all and (max-width: 320px){  
    .text__h1{
        font-size: calc(2.4rem + 6vw);
    }
    .text__p{
        font-size: calc(.8rem + 2vw);
    }
 
    .footer__list__link{
        font-size: calc(8rem + 4vw);
    }
    .footer__copyright, .footer__santi{
        font-size: calc(.4rem + 1vw);
    }
}

@media all and (max-width: 290px){
    .header__logo{
        display: none;
    } 
    .header__burgerIcon{
        margin:auto;
    } 
    .text__h1{
        font-size: calc(2rem + 6vw);
    } 
    .text__p{
        font-size: calc(.8rem + 1vw);
    }     
}

 /*para mobile, en la galeria, 'transform: scale' se activa en ':hover' directamente en vez de en ':active' (funciona mejor asi para tactil) */
@media all and (orientation: portrait) and (max-width: 1024px){
    .gallery__img:active, .gallery__img:focus, .gallery__img:hover{
        border: .5rem solid transparent;  
        animation: borderAnimation 1500ms linear infinite;
        transform: scale(1.1);
    } 
}

